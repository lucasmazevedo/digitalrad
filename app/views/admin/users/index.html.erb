      <h1 class="page-header">Administradores <%= link_to 'Cadastrar Novo', new_admin_user_path, class: "btn btn-primary pull-right" %></h1>
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">Lista </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>Nome</th>
                                            <th>Email</th>
                                            <th>Data Criação</th>
                                            <th>Ações</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% @users.each do |user| %>
                                          <tr>
                                            <td><%= user.id %></td>
                                            <td><%= user.name %></td>
                                            <td><%= user.email %></td>
                                            <td><%= I18n.l(user.created_at, format: :long) %></td>
                                            <td>
                                              <%= link_to 'Exibir', [:admin, user], class: "btn btn-info btn-sm" %>
                                              <%= link_to 'Editar', edit_admin_user_path(user), class: "btn btn-warning btn-sm" %>
                                              <%= link_to 'Excluir', [:admin, user], class: "btn btn-danger btn-sm", method: :delete, data: { confirm: 'Deseja deletar o registro?' } %>
                                            </td>
                                          </tr>
                                        <% end %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
            </div>
